{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "index.py"
    },
    {
      "src": "/status",
      "dest": "index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
